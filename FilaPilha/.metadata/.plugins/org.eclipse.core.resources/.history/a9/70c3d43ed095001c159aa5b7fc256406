class Aluno{
	public Aluno(int matricula, String nome) {
		matricula = this.matricula;
		nome = this.nome;
	}
	public int matricula;
	public String nome;
}

class Node {
    
    Aluno aluno;
    Node next;
    Node prev;
    
    public Node(Aluno aluno) {
        this.aluno = aluno;
        this.prev = null;
        this.next = null;
    }   
}


class LinkedList {

}
